# ----------------------------------------
# COMMON PROPERTIES
# ----------------------------------------

smi-swagger:
  # 标题
  title: #{title}
  # 描述信息
  description: #{description}
  # 服务条款网址
  terms-service-url: #{terms-service-url}
  # 联系方式
  contact: #{contact}
  # 是否允许提供API服务,默认为TRUE
  enable: #{enable}

# ----------------------------------------
# PROFILES PROPERTIES
# ----------------------------------------

---
# Development environment
spring:
  # PROFILES
  profiles: development

smi-swagger:
  # 是否允许提供接口文档页面服务,默认为FALSE
  allow-page: #{allow-page}

---
# Test environment
spring:
  # PROFILES
  profiles: test

smi-swagger:
  # 是否允许提供接口文档页面服务,默认为FALSE
  allow-page: #{allow-page}

---
# Production environment
spring:
  # PROFILES
  profiles: production

smi-swagger:
  # 是否允许提供接口文档页面服务,默认为FALSE
  allow-page: #{allow-page}